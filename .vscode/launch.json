    "version": "0.2.0",
    "configurations": [
    {
        "command": "npm start",
        "name": "Run npm start",
        "request": "launch",
        "type": "node-terminal"
    },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server"
        },


           {
            "name": "Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:30001/chat",
            "webRoot": "${workspaceFolder}"
        },
    ]
}